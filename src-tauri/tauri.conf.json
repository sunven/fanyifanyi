{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fanyifanyi",
  "version": "0.1.9",
  "identifier": "com.fanyifanyi.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fanyifanyi",
        "width": 800,
        "height": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "fanyifanyi"
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/sunven/fanyifanyi/releases/latest/download/{{target}}-{{arch}}.json"
      ],
      "pubkey": "RWRVRKavfsFAJitXi+eZLh0BVFMBygdEB0MvJ3tDB5QFvkKMYG8e7fid"
    }
  }
}
